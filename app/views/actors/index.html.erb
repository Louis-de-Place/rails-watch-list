<div class="banner">
  <div class="container">
    <h1>Actors</h1>
  </div>
</div>
<div class="container w-75 my-4">
  <div class="row">
    <div class="d-flex justify-content-between mb-3">
      <div class="header">Top Rated</div>
    </div>
    <div class="cards">
      <% @actors.each do |a| %>
        <div class="card-trip">
          <div class="image-container">
            <img src="https://image.tmdb.org/t/p/w500<%= a.profile_url %>" />
          </div>
          <h2 class="text-truncate movie-title"><%= a.name %></h2>
        </div>
      <% end %>
    </div>
  </div>
</div>
